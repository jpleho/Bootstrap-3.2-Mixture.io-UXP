<!doctype html>
<html lang="en">
    <head>
        <link href='http://fonts.googleapis.com/css?family=Bitter:700|Open+Sans:600,400' rel='stylesheet' type='text/css'>
        <title>{% block title %}{% endblock %}</title>
        <link rel="shortcut icon" type="image/ico" href="/favicon.ico" />
        {% include "styles" %}
        {% block styles %}{% endblock %}
    </head>
    <body data-spy="scroll" data-target=".sidebar">
        <div id="wrapper">
            <div class="container">
                {% include "header" %}            
                {% block content %}{% endblock %}
                {% include "footer" %}
             </div>
        </div>
        {% block scripts %}{% endblock %}
        {% include "scripts" %}
    </body>
</html>