<!doctype html>
<html lang="en">
    <head>
        <link href='http://fonts.googleapis.com/css?family=Bitter|Lato|Open+Sans|Roboto+Slab' rel='stylesheet' type='text/css'>
        <title>{% block title %}{% endblock %}</title>
        <link rel="shortcut icon" type="image/ico" href="/favicon.ico" />
        {{ "bootstrap.css" | asset_url | stylesheet_tag }}
        {{ "style.css" | asset_url | stylesheet_tag }}
        {% block styles %}{% endblock %}
    </head>
    <body>
        <div id="wrapper">
            <div class="container">
                {% include "header" %}            
                {% block content %}{% endblock %}
                {% include "footer" %}
             </div>
        </div>
        {% block scripts %}{% endblock %}
        {% include "scripts" %}
    </body>
</html>